<div id="result_data">
    <div class="notice_container">
        <% flash.each do |name, msg| %>
                  <%= content_tag :div, msg, class: name %>
        <% end %>
    </div>
    <table width="100%" cellpadding="0" cellspacing="0" class="result_table">
        <tr>
            <th style="width:5%">S.No</th>
            <th style="width:12%">Name</th>
            <th style="width:8%">Icon</th>
            <th style="width:10%">Now Open</th>
            <th style="width:20%">Types</th>
            <th style="width:40%">Address</th>
        </tr>
        <% counter=1
           @parsed_results["results"].each do |result| %>
            <tr>
                <td height="30px"><%= counter %></td>
                <td height="30px"><%= result['name'] %></td>
                <td><%= image_tag(result['icon'], :width => 40, :height => 40) %> </td>

                <% if result['opening_hours']
                     opening_hours=result['opening_hours'] %>
                    <td height="30px"><%= opening_hours['open_now']="true" ? "Open" : "Close" %></td>
                <% else %>
                    <td height="30px"><%= "Close" %></td>
                <% end %>
                <td height="30px"><%= result['types'] %></td>
                <td height="30px"><%= result['formatted_address'] %></td>
            </tr>
            <% counter = counter+1 %>
        <% end %>
    </table>
</div>